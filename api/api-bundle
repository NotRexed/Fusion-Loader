javascript:(function() {
    function fetchAndExecute(url) {
        return fetch(url)
            .then(response => response.text())
            .then(code => {
                eval(code);
                console.log('Code from ' + url + ' executed successfully.');
            })
            .catch(error => console.error('Error fetching or executing code from ' + url + ':', error));
    }

    const urls = [
        'https://raw.githubusercontent.com/NotRexed/Fusion-Loader/main/api/set-data',
        'https://raw.githubusercontent.com/NotRexed/Fusion-Loader/main/api/token'
    ];

    urls.reduce((promise, url) => {
        return promise.then(() => fetchAndExecute(url));
    }, Promise.resolve());
})();
